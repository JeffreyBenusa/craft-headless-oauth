{
  "name": "jeff-benusa/craft-headless-oauth",
  "description": "React Native and OAuth project for learning technologies",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.8.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "jeffbenusa\\headlessoauth\\": "src/"
    }
  },
  "extra": {
    "class": "jeffbenusa\\headlessoauth\\Plugin",
    "handle": "_headless-oauth",
    "name": "Headless OAuth",
    "description": "React Native and OAuth project for learning technologies",
    "documentationUrl": "https://github.com/JeffreyBenusa/craft-headless-oauth/blob/main/README.md"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}